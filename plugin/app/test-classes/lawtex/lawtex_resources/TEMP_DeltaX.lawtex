template[TEMP_DeltaX] {
	metainfo {
		language = "pt_BR"
	}
 	body {
		declarations {
			+<a> : String {
				label = "xx"
			},
			+<b> : String,
			+|c| : Vector[String] {
				label = "yy"
			},
			+<d> : struct[WW] {
				fields {
					+[z] : String,
					+[w] : String {
						label = "ww"
					}
				}
			}
		}
		operations {
			print <a> & <b> & <c> & <d>
		}
 	}
}