template[TEMP_BasicTests] {
   metainfo {
      language = "pt_BR"
      name = "Gualberto"
   }
   head {}
   body {
      declarations {
         +<doc> : Document[TEMP_BasicTests] {
            request = 1
            order = 1
         },
         +|vectdoc| : Vector[Document[TEMP_BasicTests]] {
            request = 1
            order = 1
         },
         +<a> : *StructAliasBasicTests1 {
            order = 1
         },
         +<atom> : String,
         +#i# : Integer {
            value = 0
         }
      }
      operations {
         print <a.doc> & "\n",
         print <a.doc$Body:atom> & "\n",
         print <a.doc$BRC_AliasBranchDoc:o> & "\n",
         print <a.vectdoc{#i#}$Body:atom> & "\n",
         print <a.vectdoc{0}$BRC_AliasBranchDoc:o> & "\n",
         print <doc> & "\n",
         print <doc$Body:atom> & "\n",
         print <doc$BRC_AliasBranchDoc:o> & "\n",
         print |vectdoc{0}| & "\n",
         print <vectdoc{0}$Body:atom> & "\n",
         print <vectdoc{0}$BRC_AliasBranchDoc:o> & "\n",
         use *branch[BRC_AliasBranchDoc]
      }
   }
}

*struct[StructAliasBasicTests1] {
   name = "Teste"
   request = "REQ"
   help = "HLP"
   fields {
      +|vectdoc| : Vector[Document[TEMP_BasicTests]] {
         request = 1
         order = 1
      },
      +[doc] : Document[TEMP_BasicTests] {
         request = 1
         order = 1
      },
      +[atom] : String
   }
}

*branch[BRC_AliasBranchDoc] {
   declarations {
      +<o> : String
   }
   operations {
      print <o>
   }
}