template[TEMP_Filter] {
   metainfo {
      language = "pt_BR"
   }
   head {
      title = "AAABBB"
   }
   body {
      declarations {
         +|v1| : Vector[Integer],
         struct[FilterElemtn] {
            fields {
               +[a] : String,
               +[b] : Boolean,
               +[c] : Integer
            }
         },
         +|v2| : Vector[FilterElemtn] {
            itemsOnPage = 3
         }
      }
      operations {
         print |v1|.filter([this] == "x"),
         print |v2|.filter([this.a] == "x"),
         print |v2|.filter([this.b]),
         print |v2|.filter(NOT [this.b]),
         print |v2|.filter([this.c] > 0)
      }
   }
}