template[TEMP_FOR]{
    metainfo{
        language="pt_BR"
    }
    head{
        title="Template FOR"
    }
    body{
        declarations {
            +<variable> : Integer,
            +|vector| : Vector[String] {
                lower = 4
                upper = 5
            }
        }
        operations {
            foreach(<elem> IN [1:10]) {
                print <elem> & " "
            },
            print "\n",
            foreach(<elem> IN [1:<variable>]) {
                print <elem> & " "
            },
            print "\n",
            foreach(<elem> IN |vector|) {
                print <elem> & " "
            },
            print "\n",
            foreach(<elem> IN [0:|vector|.size()]) {
                print <elem> & " "
            },
            print "\n",
            foreach(<elem> IN [|vector|.lower - 1 : |vector|.upper]) {
                print |vector{<elem>}| & " "
            }
        }
    }
}