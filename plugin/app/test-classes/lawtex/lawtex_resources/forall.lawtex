template[TEMP_ForallExists] {
	metainfo {
		language = "pt_BR"
	}
	head {
		title = "Template Forall/Exists"
	}
	body {
		declarations {
			+|vector| : Vector[String] {
				name = "V"
				request = "V"
			}
		}
		operations {
			if (EXISTS <proxy> IN |vector| : <proxy> == "...") {
				print "Pediu outros"
			} elseif (FORALL <proxy> IN |vector| : <proxy> ~= "a") {
				print "nem todos s√£o iguais a 'a'"
			} else {
				print "qualquer coisa"
			}
		}
	}
}
