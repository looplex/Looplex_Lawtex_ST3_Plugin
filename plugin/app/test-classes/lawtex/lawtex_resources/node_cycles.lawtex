template[TEMP_NODECYCLES] {
	metainfo {
		language = "pt_BR"
	}
 	head {
 		title=""
 	}
 	body {
		operations {
			use *dependency[DEP_AliasDependencyCycle]
		}
 	}
}

*dependency[DEP_AliasDependencyCycle] {
	declarations {
		+<a> : Integer
	}
	operation {
		if ( <a> > 0 ) {
 			print {
 				version = "Okay.. Você selecionou um número positivo"
 			}
		} else {
			use *dependency[DEP_AliasDependencyCycle] where ( <a> = <a>.clear() )
		}
	}
}

