template[TEMP_FRUITS] {
	metainfo {
		language = "pt_BR"
	}
	head {
		title = "Template Frutas"
	}
	body {
		declarations {
			+<objetoOffline> : StructOffline,
			struct[StructOffline] {
				name = "Struct Exemplo"
				request = "Zetacaretone"
				fields {
					-[f1] : String {
						name = "Fech"
						key = true
					}
				}
			},
			+<objetoInline> : struct[StructInline] {
				name = "Struct Inline"
				request = "Zicaaa"
				fields {
					-[fqqr] : String {
						name = "fqqr"
					}
				}
			}
		}
		operations {
			print <objetoOffline> &  <objetoInline>
		}
	}
}